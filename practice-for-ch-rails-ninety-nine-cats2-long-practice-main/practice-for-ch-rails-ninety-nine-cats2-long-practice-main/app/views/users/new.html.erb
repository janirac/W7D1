<h1>Sign Up</h1>
<!--<%= render 'form', user: @user, type: :new %>>-->


<form action="<%= users_url %>" method="POST">
  <label for="username">Username:
    <input type="text" id="username" name="user[username]" value="<%= @user.username %>">
  </label>
  <br/>
  <label for="password">Password:
    <input type="text" id="password" name="user[password]" value="<%= @user.password %>">
  </label>
  <br/>
</form>